export DOCKER_REPO_PREFIX=lukewaite

aws1(){
	docker run -it --rm \
		-v "${HOME}/.aws:/root/.aws" \
		-v "${HOME}/Downloads:/root/Downloads" \
        -v "$(pwd):/root/aws-workspace" \
        -w "/root/aws-workspace" \
		--log-driver none \
		--name aws-$(date "+%s") \
		${DOCKER_REPO_PREFIX}/awscli "$@"
}

aws(){
	docker run -it --rm \
		-v "${HOME}/.aws:/root/.aws" \
		-v "${HOME}/.kube:/root/.kube" \
		-v "${HOME}/Downloads:/root/Downloads" \
        -v "$(pwd):/root/aws-cli-workspace" \
        -w "/root/aws-cli-workspace" \
		--log-driver none \
		--name aws-$(date "+%s")     \
		amazon/aws-cli "$@"
}

aws-iamlive(){
	docker run -it --rm \
		-v "${HOME}/.iamlive/:/root/.iamlive/" \
		-v "${HOME}/.aws:/root/.aws" \
		-v "${HOME}/.kube:/root/.kube" \
		-v "${HOME}/Downloads:/root/Downloads" \
        -v "$(pwd):/root/aws-cli-workspace" \
        -w "/root/aws-cli-workspace" \
        -e HTTP_PROXY=http://host.docker.internal:10080 \
        -e HTTPS_PROXY=http://host.docker.internal:10080 \
        -e AWS_CA_BUNDLE=/root/.iamlive/ca.pem \
        -e AWS_CSM_PORT=31000 \
        -e AWS_CSM_HOST=host.docker.internal \
		--log-driver none \
		--name aws-$(date "+%s")     \
		amazon/aws-cli "$@"
}

aws-shell(){
	docker run -it --rm \
		-v "${HOME}/.aws:/root/.aws" \
		-v "${HOME}/Downloads:/root/Downloads" \
        -v "$(pwd):/root/aws-workspace" \
        -w "/root/aws-workspace" \
		--log-driver none \
		--name aws-shell \
		${DOCKER_REPO_PREFIX}/aws-shell "$@"
}

eb(){
	docker run -it --rm \
		-v "${HOME}/.aws:/root/.aws" \
		-v "${HOME}/.ssh/:/root/.ssh/" \
		-v "${HOME}/Downloads:/root/Downloads" \
		-v /var/run/docker.sock:/var/run/docker.sock \
		-v "$(pwd):/root/aws-elasticbeanstalk-workspace" \
		--log-driver none \
		--name aws-elasticbeanstalk-$(date "+%s") \
		${DOCKER_REPO_PREFIX}/aws-elasticbeanstalk "$@"
}

fargate(){
	docker run -it --rm \
		-v "${HOME}/.aws:/root/.aws" \
		-v "${HOME}/.ssh/:/root/.ssh/" \
		-v "${HOME}/Downloads:/root/Downloads" \
		-v /var/run/docker.sock:/var/run/docker.sock \
		-v "$(pwd):/root/aws-fargate-workspace" \
		-e AWS_PROFILE=${AWS_PROFILE} \
		--log-driver none \
		--name aws-fargate-$(date "+%s") \
		${DOCKER_REPO_PREFIX}/aws-fargate-cli "$@"
}

mssh(){
	docker run -it --rm \
		-v "${HOME}/.aws:/root/.aws" \
		-v "${HOME}/.ssh/:/root/.ssh/" \
		--log-driver none \
		--name mssh-$(date "+%s") \
		${DOCKER_REPO_PREFIX}/aws-ec2-instance-connect-cli "$@"
}

msftp(){
        docker run -it --rm \
                -v "${HOME}/.aws:/root/.aws" \
                -v "${HOME}/.ssh/:/root/.ssh/" \
                -v "${HOME}/Downloads:/root/Downloads" \
		-w "/root/Downloads" \
		--entrypoint msftp \
                --log-driver none \
                --name msftp-$(date "+%s") \
                ${DOCKER_REPO_PREFIX}/aws-ec2-instance-connect-cli "$@"
}

nnn(){
	docker run -it --rm \
        -v "$(pwd):/volume" \
        -w "/volume" \
		--log-driver none \
		--name nnn-$(date "+%s") \
		${DOCKER_REPO_PREFIX}/nnn "$@"
}

sam(){
	docker run -i --rm \
		-v "${HOME}/.aws:/root/.aws" \
		-v "${HOME}/.ssh/:/root/.ssh/" \
		-v "${HOME}/Downloads:/root/Downloads" \
		-v /var/run/docker.sock:/var/run/docker.sock \
		-v "$(pwd):$(pwd)" \
		-w "$(pwd)" \
		-e AWS_PROFILE=${AWS_PROFILE} \
		-e AWS_DEFAULT_REGION=${AWS_DEFAULT_REGION} \
		--log-driver none \
		--name aws-sam-$(date "+%s") \
		${DOCKER_REPO_PREFIX}/aws-sam-cli "$@"
}

yq() {
  docker run --rm -i -v "${PWD}":/workdir mikefarah/yq yq "$@"
}

xq() {
  docker run --rm -i -v "${PWD}":/workdir -w /workdir alpine/xml xq "$@"
}

youtube-dl() {
        docker run -it --rm \
                -v "${HOME}/Downloads:/downloads" \
		-w "/downloads" \
                --log-driver none \
                --name youtube-dl-$(date "+%s") \
                ${DOCKER_REPO_PREFIX}/youtube-dl "$@"
}

iamlive(){
    docker run --rm \
      -p 10080:10080 \
	  -v "${HOME}/.iamlive/:/home/appuser/.iamlive/" \
      --name iamlive \
      -it ${DOCKER_REPO_PREFIX}/iamlive \
      --mode proxy \
      --bind-addr 0.0.0.0:10080
}